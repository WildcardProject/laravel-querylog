{
    "name": "programming_cat/laravel-querylog",
    "description": "output query log with 'querylog' channel",
    "type": "library",
    "keywords": [
        "wildcard",
        "programming_cat",
        "laravel",
        "package",
        "query",
        "sql",
        "log"
    ],
    "require": {
        "php": "~7.1"
    },
    "require-dev": {
        "laravel/framework": "5.8.*|^6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "T.Yamazaki",
            "email": "takanori@it-gypsy.jp",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "programming_cat\\QueryLog\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "programming_cat\\QueryLog\\Providers\QueryLogServiceProvider"
            ]
        }
    }
